{"ast":null,"code":"export default {\n  data() {\n    return {\n      isCollapse: false,\n      MenuList: [{\n        title: '首页',\n        index: '/home/index'\n      }, {\n        title: '演员管理',\n        children: [{\n          sub_title: '演员列表',\n          index: '/home/director-list'\n        }, {\n          sub_title: '新增演员',\n          index: '/home/actor-list'\n        }]\n      }, {\n        title: '导演管理',\n        children: [{\n          sub_title: '导演列表',\n          index: '/home/director-list'\n        }, {\n          sub_title: '新增导演',\n          index: '/home/director-list'\n        }]\n      }, {\n        title: '电影管理',\n        children: [{\n          sub_title: '电影列表',\n          index: '/home/movie-list'\n        }, {\n          sub_title: '新增电影',\n          index: '/home/movie-list'\n        }]\n      }, {\n        title: '电影院管理',\n        children: [{\n          sub_title: '电影院列表',\n          index: '/home/actor-list'\n        }, {\n          sub_title: '新增电影院',\n          index: '/home/cinema-list'\n        }]\n      }]\n    };\n  },\n  created() {\n    console.log(this.$route);\n  }\n};","map":{"version":3,"names":["data","isCollapse","MenuList","title","index","children","sub_title","created","console","log","$route"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div>\n    <el-container style=\"height: 100vh\">\n      <el-aside\n        class=\"aside\"\n        :style=\"{ width: `${isCollapse ? 65 : 240}px`, transition: '0.4s' }\"\n      >\n        <el-menu\n          :default-active=\"this.$route.path || 'index'\"\n          router\n          :default-openeds=\"['1']\"\n          background-color=\"#345\"\n          text-color=\"#bbb\"\n          :style=\"{ height: '100vh' }\"\n          :collapse=\"isCollapse\"\n          class=\"el-menu-vertical-demo\"\n        >\n          <el-menu-item>\n            <img\n              width=\"30px\"\n              src=\"../assets/logo.svg\"\n              alt=\"\"\n            />\n            <template slot=\"title\">\n              <span\n                style=\"font-size: 1.2em; color: #efefef; font-weight: bold\">&nbsp;&nbsp;百慕大影城后台管理</span>\n            </template>\n          </el-menu-item>\n          <template v-for=\"(item,index) in MenuList\">\n            <el-menu-item\n              v-if=\"!item.children\"\n              :index=\"item.index\"\n              :key=\"item.title\"\n            >\n              <i class=\"el-icon-message\"></i>\n              <template slot=\"title\">\n              </template>\n            </el-menu-item>\n            <el-submenu\n              v-else\n              index=\"index\"\n              :key=\"item.title\"\n            >\n              <template slot=\"title\">\n                <i class=\"el-icon-location\"></i>\n                <span slot=\"title\">{{ item.title}}</span>\n              </template>\n              <el-menu-item\n                v-for=\"i in item.children\"\n                :key=\"i.sub_title\"\n                :index=\"i.index\"\n              >\n                <i class=\"el-icon-notebook-2\"></i>\n                <span slot=\"title\">{{ i.sub_title }}</span>\n              </el-menu-item>\n            </el-submenu>\n          </template>\n        </el-menu>\n      </el-aside>\n\n      <el-container>\n        <el-header class=\"header\">\n          <i\n            v-show=\"!isCollapse\"\n            class=\"el-icon-s-fold\"\n            @click=\"isCollapse = true\"\n          ></i>\n          <i\n            v-show=\"isCollapse\"\n            class=\"el-icon-s-unfold\"\n            @click=\"isCollapse = false\"\n          ></i>\n          <span>未登录</span>\n        </el-header>\n\n        <el-main>\n          <!-- 二级占位符 -->\n          <router-view></router-view>\n        </el-main>\n      </el-container>\n    </el-container>\n  </div>\n</template>\n\n<script>\nexport default {\n  data () {\n    return {\n      isCollapse: false,\n      MenuList: [\n        { title: '首页', index: '/home/index' },\n        {\n          title: '演员管理',\n          children: [\n            { sub_title: '演员列表', index: '/home/director-list' },\n            { sub_title: '新增演员', index: '/home/actor-list' },\n          ],\n        },\n        {\n          title: '导演管理',\n          children: [\n            { sub_title: '导演列表', index: '/home/director-list' },\n            { sub_title: '新增导演', index: '/home/director-list' },\n          ],\n        },\n        {\n          title: '电影管理',\n          children: [\n            { sub_title: '电影列表', index: '/home/movie-list' },\n            { sub_title: '新增电影', index: '/home/movie-list' },\n          ],\n        },\n        {\n          title: '电影院管理',\n          children: [\n            { sub_title: '电影院列表', index: '/home/actor-list' },\n            { sub_title: '新增电影院', index: '/home/cinema-list' },\n          ],\n        },\n      ],\n    };\n  },\n  created () {\n    console.log(this.$route);\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  border-bottom: 1px solid #ccc;\n\n  i {\n    font-size: 1.3em;\n  }\n}\n.aside::-webkit-scrollbar {\n  display: none;\n}\n.el-menu-vertical-demo:not(.el-menu--collapse) {\n  width: 240px;\n  min-height: 400px;\n}\n</style>\n"],"mappings":"AAqFA;EACAA,KAAA;IACA;MACAC,UAAA;MACAC,QAAA,GACA;QAAAC,KAAA;QAAAC,KAAA;MAAA,GACA;QACAD,KAAA;QACAE,QAAA,GACA;UAAAC,SAAA;UAAAF,KAAA;QAAA,GACA;UAAAE,SAAA;UAAAF,KAAA;QAAA;MAEA,GACA;QACAD,KAAA;QACAE,QAAA,GACA;UAAAC,SAAA;UAAAF,KAAA;QAAA,GACA;UAAAE,SAAA;UAAAF,KAAA;QAAA;MAEA,GACA;QACAD,KAAA;QACAE,QAAA,GACA;UAAAC,SAAA;UAAAF,KAAA;QAAA,GACA;UAAAE,SAAA;UAAAF,KAAA;QAAA;MAEA,GACA;QACAD,KAAA;QACAE,QAAA,GACA;UAAAC,SAAA;UAAAF,KAAA;QAAA,GACA;UAAAE,SAAA;UAAAF,KAAA;QAAA;MAEA;IAEA;EACA;EACAG,QAAA;IACAC,OAAA,CAAAC,GAAA,MAAAC,MAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}