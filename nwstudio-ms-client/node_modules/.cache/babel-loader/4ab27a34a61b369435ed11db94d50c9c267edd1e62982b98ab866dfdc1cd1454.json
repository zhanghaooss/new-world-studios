igger: 'blur' }],\r\n        room_type: [{ required: true, message: '必填', trigger: 'change' }],\r\n      },\r\n      type_option: [],\r\n      loading: false,\r\n      cinemaRoomData:[]\r\n    };\r\n  },\r\n  methods: {\r\n    getTypesList () {\r\n      httpApi.cinemaRoomApi.queryAllTypes().then((res) => {\r\n        this.type_option = res.data.data;\r\n      });\r\n    },\r\n    remoteMethod () { },\r\n    submit () {\r\n      this.$refs['form'].validate((valid) => {\r\n        if (valid) {\r\n          this.form.room_type = String(this.form.room_type);\r\n          httpApi.cinemaRoomApi.add(this.form).then((res) => {\r\n            console.log(res);\r\n            if (res.data.code == 200) {\r\n              this.$message({\r\n                message: '添加成功成功',\r\n                type: 'success',\r\n              });\r\n            }\r\n          });\r\n          dialogFormVisible = false;\r\n        }\r\n      });\r\n    },\r\n    getRoomList(){\r\n      httpApi.cinemaRoomApi.getRoomList({id:this.id}).then(res=>{console.log(res)})\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getTypesList();\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped></style>\r\n"],"mappings":"AAgIA,OAAAA,OAAA;AACA;EACAC,KAAA;IACA;MACAC,EAAA,OAAAC,MAAA,CAAAC,MAAA,CAAAC,QAAA;MACAC,iBAAA;MACAC,IAAA;QACAC,SAAA;QACAC,SAAA;QACAC,eAAA,OAAAP,MAAA,CAAAC,MAAA,CAAAC;MACA;MACAM,KAAA;QACAH,SAAA;UAAAI,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAL,SAAA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;MACAC,WAAA;MACAC,OAAA;MACAC,cAAA;IACA;EACA;EACAC,OAAA;IACAC,aAAA;MACAnB,OAAA,CAAAoB,aAAA,CAAAC,aAAA,GAAAC,IAAA,CAAAC,GAAA;QACA,KAAAR,WAAA,GAAAQ,GAAA,CAAAtB,IAAA,CAAAA,IAAA;MACA;IACA;IACAuB,aAAA;IACAC,OAAA;MACA,KAAAC,KAAA,SAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAArB,IAAA,CAAAE,SAAA,GAAAoB,MAAA,MAAAtB,IAAA,CAAAE,SAAA;UACAT,OAAA,CAAAoB,aAAA,CAAAU,GAAA,MAAAvB,IAAA,EAAAe,IAAA,CAAAC,GAAA;YACAQ,OAAA,CAAAC,GAAA,CAAAT,GAAA;YACA,IAAAA,GAAA,CAAAtB,IAAA,CAAAgC,IAAA;cACA,KAAAC,QAAA;gBACArB,OAAA;gBACAsB,IAAA;cACA;YAC