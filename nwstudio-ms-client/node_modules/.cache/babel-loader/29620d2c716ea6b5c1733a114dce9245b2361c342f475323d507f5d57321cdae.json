tTip) => {\r\n\t\t\t\t\treturn inputTip.value.toLowerCase().indexOf(queryString.toLowerCase()) === 0;\r\n\t\t\t\t};\r\n\t\t\t},\r\n\t\t\t//点击搜索的建议\r\n\t\t\thandleSelect(item) {\r\n\t\t\t\tthis.getListByName(item.director_name);\r\n\t\t\t},\r\n\t\t\t//删除\r\n\t\t\tDeleteDirector(event, id) {\r\n\t\t\t\tmyaxios.post('http://localhost:3010/movie-director/del', { id }).then((res) => {\r\n\t\t\t\t\tif (res.data.code == 200) {\r\n\t\t\t\t\t\tthis.searchAc();\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\t\t\t},\r\n\t\t},\r\n\t\tmounted() {\r\n\t\t\t//初始化列表\r\n\t\t\tmyaxios\r\n\t\t\t\t.get('http://localhost:3010/movie-directors', { page: 1, pagesize: 100 })\r\n\t\t\t\t.then((res) => {\r\n\t\t\t\t\tlet result = res.data.data;\r\n\t\t\t\t\tthis.directorLists = result.map((director) => ({\r\n\t\t\t\t\t\t...director,\r\n\t\t\t\t\t\tvalue: director.director_name,\r\n\t\t\t\t\t}));\r\n\t\t\t\t\tthis.resList = this.directorLists;\r\n\t\t\t\t\tthis.inputTip = this.directorLists;\r\n\t\t\t\t});\r\n\t\t},\r\n\t};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\t.acList {\r\n\t\tpadding-top: 20px;\r\n\t\tdisplay: flex;\r\n\t\tflex-wrap: wrap;\r\n\t\t.director {\r\n\t\t\tpadding: 5px;\r\n\t\t\twidth: 100px;\r\n\t\t\tdisplay: flex;\r\n\t\t\tflex-direction: column;\r\n\t\t\timg {\r\n\t\t\t\twidth: 100%;\r\n\t\t\t\tborder-radius: 5px;\r\n\t\t\t}\r\n\t\t\tp {\r\n\t\t\t\ttext-align: center;\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</style>\r\n"],"mappings":"AA6CA,OAAAA,OAAA;AACA,OAAAC,MAAA;AACA;EACAC,KAAA;IACA;MACA;AACA;AACA;AACA;AACA;MACAC,aAAA;MACAC,QAAA;MACAC,OAAA;MACAC,MAAA;IACA;EACA;EACAC,UAAA;IACAN;EACA;EACAO,OAAA;IACA;IACAC,cAAAC,IAAA;MACAV,OAAA,CACAW,IAAA;QAAAD,IAAA,EAAAA;MAAA,GACAE,IAAA,CAAAC,GAAA;QACA,KAAAR,OAAA,GAAAQ,GAAA,CAAAX,IAAA,CAAAA,IAAA;MACA,GACAY,KAAA,CAAAC,GAAA,IAAAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;IACA;IACAG,YAAA;MACAF,OAAA,CAAAC,GAAA;MACA,SAAAX,MAAA;QACA,KAAAG,aAAA,MAAAH,MAAA,CAAAa,IAAA;MACA;QACA,KAAAd,OAAA,QAAAF,aAAA;MACA;IACA;IACA;IACAiB,SAAA;MACA,SAAAd,MAAA;QACA,KAAAG,aAAA,MAAAH,MAAA,CAAA