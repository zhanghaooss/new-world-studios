\t\t\t<el-select v-model=\"form.tags\" style=\"width: 100%\" placeholder=\"请选择\" multiple>\r\n\t\t\t\t\t<el-option\r\n\t\t\t\t\t\tv-for=\"item in tags\"\r\n\t\t\t\t\t\t:key=\"item.id\"\r\n\t\t\t\t\t\t:label=\"item.tagname\"\r\n\t\t\t\t\t\t:value=\"item.tagname\"\r\n\t\t\t\t\t>\r\n\t\t\t\t\t</el-option>\r\n\t\t\t\t</el-select>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item>\r\n\t\t\t\t<el-button type=\"primary\" @click=\"submit\">更新电影院</el-button>\r\n\t\t\t</el-form-item>\r\n\t\t</el-form>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\timport AMapLoader from '@amap/amap-jsapi-loader';\r\n\timport httpApi from '@/http';\r\n\r\n\texport default {\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\ttags: [], // 保存所有的标签\r\n\r\n\t\t\t\tform: {\r\n\t\t\t\t\tcinema_name: '',\r\n\t\t\t\t\taddress: '',\r\n\t\t\t\t\tprovince: '',\r\n\t\t\t\t\tcity: '',\r\n\t\t\t\t\tdistrict: '',\r\n\t\t\t\t\tlongitude: '',\r\n\t\t\t\t\tlatitude: '',\r\n\t\t\t\t\ttags: [],\r\n\t\t\t\t},\r\n\t\t\t\trules: {\r\n\t\t\t\t\tcinema_name: [{ required: true, message: '必填', trigger: 'blur' }],\r\n\t\t\t\t\taddress: [{ required: true, message: '必填', trigger: 'blur' }],\r\n\t\t\t\t\tprovince: [{ required: true, message: '必填', trigger: 'blur' }],\r\n\t\t\t\t\tcity: [{ required: true, message: '必填', trigger: 'blur' }],\r\n\t\t\t\t\tdistrict: [{ required: true, message: '必填', trigger: 'blur' }],\r\n\t\t\t\t\tlongitude: [{ required: true, message: '必填', trigger: 'blur' }],\r\n\t\t\t\t\tlatitude: [{ required: true, message: '必填', trigger: 'blur' }],\r\n\t\t\t\t\ttags: [{ required: true, message: '必填', trigger: 'change' }],\r\n\t\t\t\t},\r\n\t\t\t};\r\n\t\t},\r\n\r\n\t\tmethods: {\r\n\t\t\t// 获取要更新的电影院信息\r\n\t\t\tgetCinemaData() {\r\n\t\t\t\thttpApi.cinemaApi.queryCinemaById(this.$route.query.id).then((res) => {\r\n\t\t\t\t\tconsole.log('电影院', res);\r\n\t\t\t\t});\r\n\t\t\t},\r\n\t\t\t// 执行提交表单\r\n\t\t\tsubmit() {\r\n\t\t\t\tthis.$refs['form'].validate((valid) => {\r\n\t\t\t\t\tif (valid) {\r\n\t\t\t\t\t\t// 所有字段验证通过\r\n\t\t\t\t\t\t// 整理字段\r\n\t\t\t\t\t\tthis.form.tags = this.form.tags.join(',');\r\n\t\t\t\t\t\tthis.$refs['form'].clearValidate();\r\n\t\t\t\t\t\tconsole.log(this.form);\r\n\t\t\t\t\t\t// 发送新增影院请求，执行业务\r\n\t\t\t\t\t\thttpApi.cinemaApi.addCinema(this.form).then((res) => {\r\n\t\t\t\t\t\t\tif (res.data.code == 200) {\r\n\t\t\t\t\t\t\t\t// 新增成功\r\n\t\t\t\t\t\t\t\tthis.$message({\r\n\t\t\t\t\t\t\t\t\tmessage: '恭喜，影院新增成功',\r\n\t\t\t\t\t\t\t\t\ttype: 'success',\r\n\t\t\t\t\t\t\t\t});\r\n\t\t\t\t\t\t\t\tthis.$router.push('/home/cinema-list');\r\n\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\t\t\t},\r\n\r\n\t\t\t// 初始化地图\r\n\t\t\tinitMap() {\r\n\t\t\t\tAMapLoader.load({\r\n\t\t\t\t\tkey: 'cba88d9a52c127f2aac69158bb9c6e1b', // 申请好的Web端开发者Key，首次调用 load 时必填\r\n\t\t\t\t\tversion: '2.0', // 指定要加载的 JSAPI 的版本，缺省时默认为 1.4.15\r\n\t\t\t\t\tplugins: ['AMap.Geocoder'], // 需要使用的的插件列表，如比例尺'AMap.Scale'等\r\n\t\t\t\t})\r\n\t\t\t\t\t.then((AMap) => {\r\n\t\t\t\t\t\tthis.map = new AMap.Map('mapContainer', {\r\n\t\t\t\t\t\t\t//设置地图容器id\r\n\t\t\t\t\t\t\tviewMode: '3D', //是否为3D地图模式\r\n\t\t\t\t\t\t\tzoom: 11, //初始化地图级别\r\n\t\t\t\t\t\t\tcenter: [116.397505, 39.907648], //初始化地图中心点位置\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t\t// 为map绑定点击事件，获取经纬度\r\n\t\t\t\t\t\tthis.map.on('click', (e) => {\r\n\t\t\t\t\t\t\tlet lng = e.lnglat.KL; // 经度\r\n\t\t\t\t\t\t\tlet lat = e.lnglat.kT; // 纬度\r\n\t\t\t\t\t\t\tconsole.log({ lng, lat });\r\n\t\t\t\t\t\t\t// 通过经纬度，访问高德地图服务，执行逆地理编码，获取位置信息\r\n\t\t\t\t\t\t\tlet geocoder = new AMap.Geocoder();\r\n\t\t\t\t\t\t\tgeocoder.getAddress([lng, lat], (status, result) => {\r\n\t\t\t\t\t\t\t\tconsole.log(status, result);\r\n\t\t\t\t\t\t\t\t// 回填表单\r\n\t\t\t\t\t\t\t\tthis.form.address = result.regeocode.formattedAddress;\r\n\t\t\t\t\t\t\t\tlet addr = result.regeocode.addressComponent;\r\n\t\t\t\t\t\t\t\tthis.form.province = addr.province;\r\n\t\t\t\t\t\t\t\tthis.form.city = addr.city ? addr.city : addr.province;\r\n\t\t\t\t\t\t\t\tthis.form.district = addr.district;\r\n\t\t\t\t\t\t\t\tthis.form.latitude = lat;\r\n\t\t\t\t\t\t\t\tthis.form.longitude = lng;\r\n\t\t\t\t\t\t\t});\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t})\r\n\t\t\t\t\t.catch((e) => {\r\n\t\t\t\t\t\tconsole.log(e);\r\n\t\t\t\t\t});\r\n\t\t\t},\r\n\t\t},\r\n\r\n\t\tmounted() {\r\n\t\t\tthis.initMap();\r\n\t\t\tthis.getCinemaData();\r\n\t\t\t// 加载所有的影院标签\r\n\t\t\thttpApi.cinemaApi.queryCinemaTags().then((res) => {\r\n\t\t\t\tconsole.log(res);\r\n\t\t\t\tthis.tags = res.data.data;\r\n\t\t\t});\r\n\t\t},\r\n\t};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped></style>\r\n"],"mappings":";AA+CA,OAAAA,UAAA;AACA,OAAAC,OAAA;AAEA;EACAC,KAAA;IACA;MACAC,IAAA;MAAA;;MAEAC,IAAA;QACAC,WAAA;QACAC,OAAA;QACAC,QAAA;QACAC,IAAA;QACAC,QAAA;QACAC,SAAA;QACAC,QAAA;QACAR,IAAA;MACA;MACAS,KAAA;QACAP,WAAA;UAAAQ,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAT,OAAA;UAAAO,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAR,QAAA;UAAAM,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAP,IAAA;UAAAK,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAN,QAAA;UAAAI,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAL,SAAA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAJ,QAAA;UAAAE,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAZ,IAAA;UAAAU,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;IACA;EACA;EAEAC,OAAA;IACA;IACAC,cAAA;MACAhB,OAAA,CAAAiB,SAAA,CAAAC,eAAA,MAAAC,MAAA,CAAAC,KAAA,CAAAC,EAAA,EAAAC,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,QAAAF,GAAA;MACA;IACA;IACA;IACAG,OAAA;MACA,KAAAC,KAAA,SAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA;UACA;UACA,KAAA1B,IAAA,CAAAD,IAAA,QAAAC,IAAA,CAAAD,IAAA,CAAA4B,IAAA;UACA,KAAAH,KAAA,SAAAI,aAAA;UACAP,OAAA,CAAAC,GAAA,MAAAtB,IAAA;UACA;UACAH,OAAA,CAAAiB,SAAA,CAAAe,SAAA,MAAA7B,IAAA,EAAAmB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAtB,IAAA,CAAAgC,IAAA;cACA;cACA,KAAAC,QAAA;gBACArB,OAAA;gBACAsB,IAAA;cACA;cACA,KAAAC,OAAA,CAAAC,IAAA;YACA;UACA;QACA;MACA;IACA;IAEA;IACAC,QAAA;MACAvC,UAAA,CAAAwC,IAAA;QACAC,GAAA;QAAA;QACAC,OAAA;QAAA;QACAC,OAAA;MACA,GACApB,IAAA,CAAAqB,IAAA;QACA,KAAAC,GAAA,OAAAD,IAAA,CAAAE,GAAA;UACA;UACAC,QAAA;UAAA;UACAC,IAAA;UAAA;UACAC,MAAA;QACA;QACA;QACA,KAAAJ,GAAA,CAAAK,EAAA,UAAAC,C